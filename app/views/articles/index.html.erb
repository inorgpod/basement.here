<h1>do markdown</h1>
<% if current_user%>
  <div class="btn"><%= link_to "New Post", new_article_path %></div>
<%end%>
<% @articles.each do |article|%>

    <h2 class = 'title'> <%= link_to article.title, article %> </h2>
    <p class = 'date'> <%=article.created_at.strftime ('%B, %d, %Y')%> </p>

<%end%>