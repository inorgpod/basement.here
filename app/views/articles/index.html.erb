
<% if current_user%>
  <div class="btn"><%= link_to "New Post", new_article_path %></div>
<%end%>
<% @articles.each do |article|%>
  <div class= row>
    <div class= arti-wrap>
      <div class="arti-title">
        
        <div class="zig-date-bottom zig-date-top">
          <div class = 'date'> <%=article.created_at.strftime ('%B, %d, %Y')%> </div>
        </div>
          <div class = 'title'> <%= link_to article.title, article %> </div>
      </div>

      <div class="arti-date">

      </div>

  
    </div>
  </div>
<%end%>